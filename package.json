{
  "name": "options-txt",
  "description": "An extension which provides data concerning Minecraft's options.txt file.",
  "author": {
    "name": "AdamRaichu",
    "url": "https://github.com/AdamRaichu"
  },
  "version": "1.0.0",
  "main": "./extension.js",
  "dependencies": {},
  "activationEvents": [
    "onLanguage:options-txt"
  ],
  "contributes": {
    "languages": [
      {
        "id": "options-txt",
        "aliases": [
          "options-txt"
        ],
        "extensions": [
          "txt"
        ],
        "configuration": "./language-configuration.json",
        "filenames": [
          "options.txt"
        ]
      }
    ],
    "grammars": [
      {
        "language": "options-txt",
        "scopeName": "source.options-txt",
        "path": "./syntaxes/options-txt.tmLanguage."
      }
    ]
  }
}